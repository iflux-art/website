{
  "name": "official",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.11.0",
  "engines": {
    "node": ">=22.x",
    "pnpm": ">=9.11.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "clean": "rimraf .next",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
