  "scripts": {
    "dev": "next dev",
    "build": "rimraf .next/cache .next/trace && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:enhanced": "ESLINT_CONFIG_FILE=eslint.config.enhanced.mjs next lint",
    "lint:fix": "next lint --fix",
    "lint:enhanced:fix": "ESLINT_CONFIG_FILE=eslint.config.enhanced.mjs next lint --fix",
    "lint:tailwind-colors": "ESLINT_CONFIG_FILE=eslint.config.enhanced.mjs next lint --no-cache --rule 'tailwind-color-checker/no-traditional-colors: error'",
    "turbo:lint": "turbo run lint",
    "clean": "rimraf .turbo/ node_modules/ .next/",
    "clean:cache": "rimraf .next/cache node_modules/.cache .next/trace",
    "preview": "next build && next start",
    "fix-install": "node scripts/fix-install.js",
    "check-deps": "node scripts/check-deps.js",
    "prebuild": "rimraf .next/cache .next/trace",
    "build:safe": "node scripts/build.js",
    "fix-deps": "node scripts/fix-deprecated-deps.js",
    "fix-deps:simple": "node scripts/fix-deprecated-deps-simple.js",
    "fix-glob": "node scripts/fix-glob-imports.js",
    "check-deprecated": "node scripts/check-deprecated-deps.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
